<?xml version='1.0' encoding='utf-8'?>
<widget id="com.racerxsoftware.rangerdanger" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RangerDanger</name>
    <description>
        A map based application for sharing geolocations.
    </description>
    <author email="kendowney.com@gmai.com" href="http://cordova.io">
        Ken Downey
    </author>
    <content src="index.html" />
    <access origin="*" />
    <!-- Override PhoneGap default configurations here -->
    <preference name="UIWebViewBounce" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
